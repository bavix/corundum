{
  "name": "bavix/corundum",
  "description": "Bavix Corundum (CDN).",
  "keywords": [
    "project",
    "imaginarium",
    "image",
    "php",
    "resize",
    "crop",
    "contain",
    "fill",
    "gd",
    "imagick"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "bavix/config": "^1.0",
    "bavix/gearman": "^1.0",
    "bavix/laravel-helpers": "^1.0",
    "bavix/laravel-middleware-ifindex": "^1.0",
    "bavix/laravel-middleware-protocol": "^1.0",
    "bavix/laravel-worker-command": "^1.0",
    "fideloper/proxy": "~3.3",
    "intervention/image": "^2.4",
    "laravel-admin-ext/config": "^1.0",
    "laravel-admin-ext/helpers": "^1.0",
    "laravel/framework": "5.5.*",
    "laravel/passport": "^4.0",
    "laravel/tinker": "~1.0",
    "spatie/image-optimizer": "^1.0"
  },
  "require-dev": {
    "bavix/tests": "~1.0",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "nesbot/carbon": "^1.22"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
