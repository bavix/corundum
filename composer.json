{
  "name": "bavix/corundum",
  "description": "Bavix Corundum (CDN).",
  "keywords": [
    "project",
    "imaginarium",
    "image",
    "php",
    "resize",
    "crop",
    "contain",
    "fill",
    "gd",
    "imagick"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "doctrine/dbal": "^2.8",
    "fideloper/proxy": "^4.0",
    "intervention/image": "^2.4",
    "jenssegers/imagehash": "^0.5",
    "laravel/framework": "5.7.*",
    "laravel/passport": "^7.0",
    "league/color-extractor": "^0.3",
    "rinvex/attributes": "^0.0",
    "spatie/laravel-image-optimizer": "^1.3",
    "vladimir-yuldashev/laravel-queue-rabbitmq": "^7.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.5",
    "darkaonline/l5-swagger": "^5.7",
    "filp/whoops": "^2.2",
    "fzaninotto/faker": "^1.8",
    "mockery/mockery": "^1.1",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
